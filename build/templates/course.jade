extends layouts/layout_course
block content
    .container
        .courses
            .courses__left
                .courses-form
                    img.courses-form__img(src="assets/i/example.jpg")
                    form.price-study

                        .price-study__plan.price-study__plan--selected
                            input.price-study__check(type="radio" name="study" id="remotely" value="remotely" checked)
                            label(for="remotely")
                                .price-study__info
                                    .price-study__name Дистанционно
                                    .price-study__price
                                        span.price-study__price.price-study__price--old 119000 &nbsp;
                                            i.fa.fa-rub(aria-hidden="true") &nbsp;
                                        span.price-study__price.price-study__price--new 59500 &nbsp;
                                            i.fa.fa-rub(aria-hidden="true") &nbsp;
                                    .price-study__time 350 ч

                        .price-study__plan
                            input.price-study__check(type="radio" name="study" id="internally" value="internally")
                            label(for="internally")
                                .price-study__info
                                    .price-study__name Очно + Д
                                    .price-study__price
                                        span.price-study__price.price-study__price--old 119000 &nbsp;
                                            i.fa.fa-rub(aria-hidden="true") &nbsp;
                                        span.price-study__price.price-study__price--new 59500 &nbsp;
                                            i.fa.fa-rub(aria-hidden="true") &nbsp;
                                    .price-study__time +40 ч

                        .price-study__plan
                            input.price-study__check(type="radio" name="study" id="individually" value="individually")
                            label(for="individually")
                                .price-study__info
                                    .price-study__name Индивидуально + Д
                                    .price-study__price
                                        span.price-study__price.price-study__price--old 119000 &nbsp;
                                            i.fa.fa-rub(aria-hidden="true") &nbsp;
                                        span.price-study__price.price-study__price--new 59500 &nbsp;
                                            i.fa.fa-rub(aria-hidden="true") &nbsp;
                                    .price-study__time +40 ч


                        .price-study__item
                            input.price-study__text(type="text" placeholder="Имя")
                        .price-study__item
                            input.price-study__text(type="text" placeholder="E-mail")
                        .price-study__item
                            input.price-study__text(type="text" placeholder="Телефон")
                        .price-study__item.price-study__item--text
